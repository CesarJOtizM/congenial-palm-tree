{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "allowJs": true,
    "jsx": "preserve",
    "jsxImportSource": "react",
    "noEmit": true,
    "incremental": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["react", "react-dom", "node"],
    "moduleResolution": "bundler"
  }
}
